<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.e_skillsbintang">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- Set AdminActivity as the main launcher activity -->
        <activity android:name=".DaftarSeminarActivity"
            android:exported="false" />
        <activity android:name=".DetailEventActivity"
            android:exported="false" />
        <activity android:name=".DetailEvent2Activity"
            android:exported="false" />
        <activity android:name=".DetailEvent3Activity"
            android:exported="false" />
        <activity android:name=".DetailEvent4Activity"
            android:exported="false" />
        <activity android:name=".EventActivity"
            android:exported="false" />
        <activity android:name=".HomeActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- Tambahkan aktivitas lain di sini, jika ada -->

    </application>

</manifest>
